
@{
    ViewData["Title"] = "NowPlaying";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Now Playing</h2>
<table>
    @{
        String[] movies = { "Detective Pikachu", "Venom", "The Grinch" };
        <tr><th>Title</th></tr>
        foreach (String movie in movies)
        {
            {<tr><td>@movie</td></tr>}
        }
    }
</table>


