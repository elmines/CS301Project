@model UAMovie.Models.ViewModels.MovieAccountUserViewModel
@{
    ViewData["Title"] = "NowPlaying";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Now Playing</h2>
<table>
    @{
        //String[] movies = { "Detective Pikachu", "Venom", "The Grinch" };
        <tr><th>Title</th></tr>

        foreach (Movie movie in Model.movies)
        {
            {<tr><td>@Html.ActionLink(movie.Name,"GetMovie","MovieAccountUserViewModel",new { movieAccountUserViewModel = Model, movieName = movie.Name })</td></tr>}

        }
    }
</table>
<p>@Html.ActionLink("My Account", "Me", "Customer", new { user = Model.user })</p>


